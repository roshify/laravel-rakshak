{
  "name": "roshify/laravel-rakshak",
  "description": "Roles and Permissions management package for Laravel",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Roshan Poojary",
      "email": "poojariroshan4@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Roshp\\LaravelRakshak\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Roshp\\LaravelRakshak\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.0",
    "illuminate/support": "^9.0 | ^10.0"
  },
  "require-dev": {
    "pestphp/pest": "^1.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Roshp\\LaravelRakshak\\LaravelRakshakServiceProvider"
      ],
      "aliases": {
        "Rakshak": "Roshp\\LaravelRakshak\\Facades\\Rakshak"
      }
    }
  }
}
